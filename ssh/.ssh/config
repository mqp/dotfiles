Host github.com
  HostName github.com
  User git
  RequestTTY no
  PreferredAuthentications publickey
  IdentityFile ~/.ssh/polrs.ecdsa
  ControlMaster auto
  ControlPath ~/.ssh/sockets/%r@%n:%p
  ControlPersist yes
  ServerAliveInterval 60

